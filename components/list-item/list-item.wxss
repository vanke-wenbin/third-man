.list-item {
  display: flex;
  justify-content: space-between;
  padding: 44rpx;
  background-color: #fff;
  font-size: 11pt;
}

.list-item .label {
  align-self: flex-start;
  color: #454545;
  font-weight: 500;
}

.list-item .data {
  align-self: center;
  flex-basis: 70%;
  text-align: right;
  color: #909090;
  font-size: 10pt;
}
