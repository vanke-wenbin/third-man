.bill-fee-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #454545;
}
.bill-fee-item .label {
  font-size: 16px;
}
.bill-fee-item .data {
  display: flex;
  flex-direction: column;
  text-align: right;
}
.bill-fee-item .data .amount {
  font-size: 14px;
  font-weight: 600;
}
.bill-fee-item .data .not-pay {
  color: #ef7c79;
  font-size: 10px;
}
