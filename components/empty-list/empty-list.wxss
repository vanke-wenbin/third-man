.empty-list {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.empty-list-main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.empty-list-main-icon {
  width: 145px;
  height: 145px;
}

.empty-list-main-text {
  margin-top: 41px;
  color: #c9c9c9;
  font-size: 16px;
}
